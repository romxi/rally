{% set concurrency = 50 %}
{% set times = 800 %}
{% set users = 3 %}
{% set tenants = 10 %}
{
  "NeutronNetworks.create_and_delete_ports": [
  {
    "args": {
      "network_create_args": {},
      "port_create_args": {},
      "ports_per_network": 10
    },
    "runner": {
      "type": "constant",
      "concurrency": {{concurrency}},
      "times": {{times}}
    },
    "context": {
      "users": {
        "tenants": {{users}},
        "users_per_tenant": {{tenants}}
      },
      "quotas": {
        "neutron": {
          "network": -1,
          "port": -1
        }
      }
    },
    "sla": { "failure_rate": { "max": 0 } }
  }
  ]
}

